<?php

namespace Dragonlands\MovieBundle\Entity;

/**
 * Rating
 */
class Rating
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $score;

    /**
     * @var \Dragonlands\MovieBundle\Entity\Tag
     */
    private $tag;

    /**
     * @var \Dragonlands\MovieBundle\Entity\Movie
     */
    private $movie;

    /**
     * @var \Dragonlands\MovieBundle\Entity\User
     */
    private $user;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set score
     *
     * @param integer $score
     *
     * @return Rating
     */
    public function setScore($score)
    {
        $this->score = $score;

        return $this;
    }

    /**
     * Get score
     *
     * @return integer
     */
    public function getScore()
    {
        return $this->score;
    }

    /**
     * Set tag
     *
     * @param \Dragonlands\MovieBundle\Entity\Tag $tag
     *
     * @return Rating
     */
    public function setTag(\Dragonlands\MovieBundle\Entity\Tag $tag = null)
    {
        $this->tag = $tag;

        return $this;
    }

    /**
     * Get tag
     *
     * @return \Dragonlands\MovieBundle\Entity\Tag
     */
    public function getTag()
    {
        return $this->tag;
    }

    /**
     * Set movie
     *
     * @param \Dragonlands\MovieBundle\Entity\Movie $movie
     *
     * @return Rating
     */
    public function setMovie(\Dragonlands\MovieBundle\Entity\Movie $movie = null)
    {
        $this->movie = $movie;

        return $this;
    }

    /**
     * Get movie
     *
     * @return \Dragonlands\MovieBundle\Entity\Movie
     */
    public function getMovie()
    {
        return $this->movie;
    }

    /**
     * Set user
     *
     * @param \Dragonlands\MovieBundle\Entity\User $user
     *
     * @return Rating
     */
    public function setUser(\Dragonlands\MovieBundle\Entity\User $user = null)
    {
        $this->user = $user;

        return $this;
    }

    /**
     * Get user
     *
     * @return \Dragonlands\MovieBundle\Entity\User
     */
    public function getUser()
    {
        return $this->user;
    }
}
